        <Label Content="login" Height="28" HorizontalAlignment="Left" Margin="88,81,0,0" Name="label1" VerticalAlignment="Top" Width="41" />
        <Label Content="password" Height="28" HorizontalAlignment="Left" Margin="88,125,0,0" Name="label2" VerticalAlignment="Top" Width="67" />
    </Grid>
